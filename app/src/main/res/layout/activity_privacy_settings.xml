<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.setting.AccountSettings">

    <RelativeLayout
        android:id="@+id/relative_header"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_56"
        android:background="@color/colorPrimary">

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tvHeading"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginStart="@dimen/dp_15"
            android:fontFamily="@font/poppins_medium"
            android:text="Privacy Settings"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_16"
            android:visibility="visible"
            tools:ignore="RelativeOverlap" />

    </RelativeLayout>

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_below="@+id/relative_header"
        android:layout_marginTop="@dimen/dp_15"
        android:background="@color/gray_light" />

    <RelativeLayout
        android:id="@+id/story_privacy_relative"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_45"
        android:layout_below="@+id/view"
        android:layout_marginStart="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_10">

        <ImageView
            android:id="@+id/story_privacy_img"
            android:layout_width="@dimen/dp_30"
            android:layout_height="@dimen/dp_30"
            android:layout_centerVertical="true"
            android:src="@drawable/story_privacy" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_toEndOf="@+id/story_privacy_img"
            android:fontFamily="@font/poppins_regular"
            android:text="Story Privacy"
            android:textColor="@color/grey"
            android:textSize="@dimen/sp_20" />

    </RelativeLayout>

    <View
        android:id="@+id/view_story_privacy"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_below="@+id/story_privacy_relative"
        android:layout_marginTop="@dimen/dp_15"
        android:background="@color/gray_light" />

    <RelativeLayout
        android:id="@+id/rlNotification"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_45"
        android:layout_below="@+id/view_story_privacy"
        android:layout_marginStart="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_10">

        <ImageView
            android:id="@+id/notification_img"
            android:layout_width="@dimen/dp_30"
            android:layout_height="@dimen/dp_30"
            android:layout_centerVertical="true"
            android:src="@drawable/notification" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_toEndOf="@+id/notification_img"
            android:fontFamily="@font/poppins_regular"
            android:text="Notification"
            android:textColor="@color/grey"
            android:textSize="@dimen/sp_20" />

    </RelativeLayout>

    <View
        android:id="@+id/view_remove_account"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_below="@+id/rlNotification"
        android:layout_marginTop="@dimen/dp_15"
        android:background="@color/gray_light" />

    <RelativeLayout
        android:id="@+id/all_contacts_relative"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_45"
        android:layout_below="@+id/view_remove_account"
        android:layout_marginStart="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_10">

        <ImageView
            android:id="@+id/all_contacts_img"
            android:layout_width="@dimen/dp_30"
            android:layout_height="@dimen/dp_30"
            android:layout_centerVertical="true"
            android:src="@drawable/all_contacts" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_toEndOf="@+id/all_contacts_img"
            android:fontFamily="@font/poppins_regular"
            android:text="All contacts"
            android:textColor="@color/grey"
            android:textSize="@dimen/sp_20" />

    </RelativeLayout>

    <View
        android:id="@+id/view_all_contacts"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_below="@+id/all_contacts_relative"
        android:layout_marginTop="@dimen/dp_15"
        android:background="@color/gray_light" />

    <RelativeLayout
        android:id="@+id/wallpaper_Relative"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_45"
        android:layout_below="@+id/view_all_contacts"
        android:layout_marginStart="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_10">

        <ImageView
            android:id="@+id/wallpaper_img"
            android:layout_width="@dimen/dp_30"
            android:layout_height="@dimen/dp_30"
            android:layout_centerVertical="true"
            android:src="@drawable/wallpaper" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_toEndOf="@+id/wallpaper_img"
            android:fontFamily="@font/poppins_regular"
            android:text="Wallpaper"
            android:textColor="@color/grey"
            android:textSize="@dimen/sp_20" />

    </RelativeLayout>

    <View
        android:id="@+id/view_wallpaper"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_below="@+id/wallpaper_Relative"
        android:layout_marginTop="@dimen/dp_15"
        android:background="@color/gray_light" />

    <RelativeLayout
        android:id="@+id/help_Relative"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_45"
        android:layout_below="@+id/view_wallpaper"
        android:layout_marginStart="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_10">

        <ImageView
            android:id="@+id/help_img"
            android:layout_width="@dimen/dp_30"
            android:layout_height="@dimen/dp_30"
            android:layout_centerVertical="true"
            android:src="@drawable/help" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_toEndOf="@+id/help_img"
            android:fontFamily="@font/poppins_regular"
            android:text="Help"
            android:textColor="@color/grey"
            android:textSize="@dimen/sp_20" />

    </RelativeLayout>

    <View
        android:id="@+id/view_help"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_below="@+id/help_Relative"
        android:layout_marginTop="@dimen/dp_15"
        android:background="@color/gray_light" />

</RelativeLayout>